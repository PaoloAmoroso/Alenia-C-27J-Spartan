<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <title>Alenia C-27J Spartan</title>
  <line>______________________________________________</line>
  <key>
    <name>e</name>
    <desc>Open/Close crew door</desc>
  </key>
  <key>
    <name>d</name>
    <desc>Open/Close para door</desc>
  </key>
  <key>
    <name>E</name>
    <desc>Open/Close cargo door</desc>
  </key>
  <key>
    <name>L</name>
    <desc>On/Off Landing light</desc>
  </key>
  <key>
    <name>K</name>
    <desc>On/Off Taxy light</desc>
  </key>
  <key>
    <name>F</name>
    <desc>On/Off Beacon light</desc>
  </key>
  <key>
    <name>Ctrl-f</name>
    <desc>On/Off Position light</desc>
  </key>
  <key>
    <name>q</name>
    <desc>Release Flares</desc>
  </key>
  <key>
    <name>j</name>
    <desc>Parachuters</desc>
  </key>
  <key>
    <name>r</name>
    <desc>Release Water (With MAFFS)</desc>
  </key>
  <key>
    <name>Q</name>
    <desc>Reset View</desc>
  </key>
  
</PropertyList>
