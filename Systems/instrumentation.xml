<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <adf>
    <name>adf</name>
    <number>0</number>
  </adf>
  
  <adf>
    <name>adf</name>
    <number>1</number>
  </adf>
  
  <airspeed-indicator>
    <name>airspeed-indicator</name>
    <number>0</number>
    <total-pressure>/systems/pitot/total-pressure-inhg</total-pressure>
    <static-pressure>/systems/static/pressure-inhg</static-pressure>
  </airspeed-indicator>
  
  <altimeter>
    <name>altimeter</name>
    <number>0</number>
    <static-pressure>/systems/static/pressure-inhg</static-pressure>
  </altimeter>
  
  <attitude-indicator>
    <name>attitude-indicator</name>
    <number>0</number>
    <suction>/systems/vacuum/suction-inhg</suction>
  </attitude-indicator>
  
  <inst-vertical-speed-indicator>
    <name>inst-vertical-speed-indicator</name>
    <number>0</number>
    <static-pressure>/systems/static/pressure-inhg</static-pressure>
  </inst-vertical-speed-indicator>
  
  <clock>
    <name>clock</name>
    <number>0</number>
  </clock>
  
  <heading-indicator>
    <name>heading-indicator</name>
    <number>0</number>
    <suction>/systems/vacuum/suction-inhg</suction>
  </heading-indicator>
  
  <heading-indicator-dg>
    <name>heading-indicator-dg</name>
    <number>0</number>
  </heading-indicator-dg>
  
  <magnetic-compass>
    <name>magnetic-compass</name>
    <number>0</number>
  </magnetic-compass>
  
  <nav-radio>
    <name>nav</name>
    <number>0</number>
  </nav-radio>
  
  <nav-radio>
    <name>nav</name>
    <number>1</number>
  </nav-radio>
  
  <dme>
    <name>dme</name>
    <number>0</number>
  </dme>
  
  <dme>
    <name>dme</name>
    <number>1</number>
  </dme>
  
  <slip-skid-ball>
    <name>slip-skid-ball</name>
    <number>0</number>
  </slip-skid-ball>
  
  <turn-indicator>
    <name>turn-indicator</name>
    <number>0</number>
  </turn-indicator>
  
  <radar>
    <name>radar</name>
    <number>0</number>
  </radar>
  
  <navigation-display>
    <name>nd</name>
    <number>0</number>
    <symbols include="nav-display.xml"/>
  </navigation-display>
  
  <groundradar>
    <name>groundradar</name>
    <!--airport-id-source>instrumentation/groundradar/id</airport-id-source-->
    <range-source>instrumentation/groundradar/range</range-source>
  </groundradar>

  <mk-viii>
    <name>mk-viii</name>
    <number>0</number>
  </mk-viii>

  <marker-beacon>
    <name>marker-beacon</name>
    <number>0</number>
  </marker-beacon>

  <transponder>
    <name>transponder</name>
    <number>0</number>
  </transponder>

  <encoder>
    <name>encoder</name>
    <number>0</number>
  </encoder>

  <tcas>
    <name>tcas</name>
    <number>0</number>
  </tcas>

  <radar>
    <serviceable type="bool" archive="y">true</serviceable>
    <switch type="string" archive="y">off</switch>
    <mode-control type="int" archive="y">3</mode-control>
    <range type="int" archive="y">20</range>
    <mode type="string" archive="y">WX</mode>
    <heading-marker type="bool" archive="y">false</heading-marker>
    <lightning type="bool" archive="y">false</lightning>
    <display-mode type="string">ARC</display-mode>
     <display-controls>
       <WX type="bool" archive="y">false</WX>
       <data type="bool" archive="y">false</data>
       <pos type="bool" archive="y">true</pos>
     </display-controls>
  </radar>

</PropertyList>
