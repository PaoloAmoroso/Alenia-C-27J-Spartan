<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <pfd>
    <minimums-baro type="int">200</minimums-baro>
    <minimums-radio type="int">200</minimums-radio>
  </pfd>

  <mk-viii>                           <!-- http://wiki.flightgear.org/index.php/GPWS -->
    <serviceable>true</serviceable>
    <configuration-module>
      <category-1> 255 </category-1>  <!-- AIRCRAFT_MODE_TYPE_SELECT -->
      <category-4>  13 </category-4>  <!-- ALTITUDE CALLOUTS -->
      <category-8>   3 </category-8>  <!-- RADIO ALTITUDE YASIM-->
      <category-14>  2 </category-14> <!-- AUDIO OUTPUT LEVEL -->
    </configuration-module>
    <inputs>
      <arinc429>
        <decision-height-ncd type="bool">0</decision-height-ncd>
        <decision-height type="double">250</decision-height>
      </arinc429>
    </inputs>
    <speaker>
      <max-dist> 2 </max-dist>        <!-- Max. distance where speaker is heard -->
      <reference-dist> 1 </reference-dist> <!-- Distance to pilot -->
      <volume>0.8</volume>
    </speaker>
    <voice>
      <file-prefix type="string">Sounds/mk-viii/</file-prefix>
    </voice>
  </mk-viii>

  <alerts>
    <master-warning type="bool">0</master-warning>
    <master-caution type="bool">0</master-caution>
  </alerts>

  <cdu n="0">
    <serviceable type="bool">1</serviceable>
    <display>IDENT</display>
    <input type="string"/>
    <ident>
      <model>C-27J</model>
    </ident>
  </cdu>

  <cdu n="1">
    <serviceable type="bool">1</serviceable>
    <display>IDENT</display>
    <input type="string"/>
    <ident>
      <model>C-27J</model>
    </ident>
  </cdu>

  <tcas>
    <serviceable type="bool">true</serviceable>
    <inputs>
      <mode type="int">1</mode> <!--0=off,1=standby,2=TA-only,3=auto(TA/RA)-->
    </inputs>
    <voice>
      <file-prefix type="string">Aircraft/CitationX/Sounds/tcas/female/</file-prefix>
    </voice>
    <tfc n="0" type="bool">0</tfc>
    <tfc n="1" type="bool">0</tfc>
    <speaker>
      <max-dist type="double">10</max-dist>
      <reference-dist type="double">2</reference-dist>
      <volume type="double"> 1 </volume>
    </speaker>
    <range type="int">6</range>
  </tcas>

  <nd>
    <radar-texture-path type="string">Aircraft/Alenia-C-27J-Spartan/Models/Interior/Panel/Instruments/mfd-pfd/tcas-display.png</radar-texture-path>
    <symbol-texture-path type="string">Aircraft/Alenia-C-27J-Spartan/Models/Interior/Panel/Instruments/mfd-pfd/symbols.png</symbol-texture-path>
    <font>
      <color>
        <red type="float">0.7</red>
        <green type="float">0.7</green>
        <blue type="float">0.7</blue>
        <alpha type="float">1</alpha>
      </color>
      <name type="string">helvetica_bold.txf</name>
      <size type="float">14</size>
      <line-spacing type="float">0.01</line-spacing>
    </font>
    <serviceable type="bool">true</serviceable>
    <range type="int" archive="y">100</range>
    <aircraft-heading-up type="bool">true</aircraft-heading-up>
    <heading-up-deg type="float">0.01</heading-up-deg>
    <x-center type="float">0.5</x-center>
    <y-center type="float">0.5</y-center>
    <display>
      <arpt type="bool">1</arpt>
      <vor type="bool">1</vor>
      <fix type="bool">0</fix>
      <tcas type="bool">0</tcas>
    </display>
    <line-width type="double">3</line-width>
  </nd>

  <tcas-display>
    <radar-texture-path type="string">Aircraft/Alenia-C-27J-Spartan/Models/Interior/Panel/Instruments/mfd-pfd/tcas-display.png</radar-texture-path>
    <symbol-texture-path type="string">Aircraft/Alenia-C-27J-Spartan/Models/Interior/Panel/Instruments/mfd-pfd/symbols.png</symbol-texture-path>
    <font>
      <color>
        <red type="float">0.7</red>
        <green type="float">0.7</green>
        <blue type="float">0.7</blue>
        <alpha type="float">1</alpha>
      </color>
      <name type="string">helvetica_bold.txf</name>
      <size type="float">14</size>
      <line-spacing type="float">0.01</line-spacing>
    </font>
    <serviceable type="bool">true</serviceable>
    <range type="int" archive="y">6</range>
    <aircraft-heading-up type="bool">true</aircraft-heading-up>
    <heading-up-deg type="float">0.01</heading-up-deg>
    <x-center type="float">0.5</x-center>
    <y-center type="float">0.5</y-center>
    <display>
    </display>
    <line-width type="double">3</line-width>
  </tcas-display>

  <alerts>
    <gear-horn type="bool">0</gear-horn>
    <master-warning type="bool">0</master-warning>
    <master-caution type="bool">0</master-caution>
  </alerts>

  <rmu>
    <unit n="0">
      <serviceable type="bool">1</serviceable>
      <selected type="int">0</selected>
      <selected-xoffset type="int">0</selected-xoffset>
      <selected-yoffset type="int">0</selected-yoffset>
      <com-num type="int">0</com-num>
      <nav-num type="int">0</nav-num>
      <adf-num type="int">0</adf-num>
    </unit>
    <unit n="1">
      <serviceable type="bool">1</serviceable>
      <selected type="int">0</selected>
      <selected-xoffset type="int">0</selected-xoffset>
      <selected-yoffset type="int">0</selected-yoffset>
      <com-num type="int">1</com-num>
      <nav-num type="int">1</nav-num>
      <adf-num type="int">1</adf-num>
    </unit>
  </rmu>

  <airspeed-indicator>
    <index n="0" type="double">100</index>
    <index n="1" type="double">120</index>
    <ias-limit>350.0</ias-limit>
    <mach-limit>0.85</mach-limit>
    <alt-threshold>8000.0</alt-threshold>
  </airspeed-indicator>

  <kr-87>
    <inputs>
      <audio-btn>0</audio-btn>
    </inputs>
  </kr-87>

  <primus2000>
    <dc840>
      <hsi type="bool">0</hsi>
      <wx type="bool">0</wx>
      <et type="bool">0</et>
      <tcas type="bool">0</tcas>
      <acft-sys type="bool">0</acft-sys>
      <mfd-map type="bool">1</mfd-map>
      <mfd-wx type="bool">0</mfd-wx>
      <norm type="bool">0</norm>
      <abn type="bool">0</abn>
      <emer type="bool">0</emer>
      <skp type="bool">0</skp>
      <pag type="bool">0</pag>
      <rcl type="bool">0</rcl>
      <ent type="bool">0</ent>
    </dc840>
    <sc840>
      <nav1ptr type="double">0</nav1ptr>
      <nav2ptr type="double">0</nav2ptr>
    </sc840>
    <eicas>
      <serviceable type="bool">1</serviceable>
    </eicas>
    <mfd>
      <menu-num type="int">0</menu-num>
    </mfd>
    <pfd>
    </pfd>
  </primus2000>

  <transponder>
    <inputs>
      <knob-mode type="int">5</knob-mode>
      <ident-run type="double">0.0</ident-run>
    </inputs>
  </transponder>

  <nav n="0">
    <nav-id type="string"></nav-id>
  </nav>

  <nav n="1">
    <nav-id type="string"></nav-id>
  </nav>

  <radar>
    <serviceable type="bool" archive="y">true</serviceable>
    <switch type="string" archive="y">off</switch>
    <mode-control type="int" archive="y">3</mode-control>
    <range type="int" archive="y">20</range>
    <mode type="string" archive="y">WX</mode>
    <heading-marker type="bool" archive="y">false</heading-marker>
    <lightning type="bool" archive="y">false</lightning>
    <display-mode type="string">ARC</display-mode>
     <display-controls>
       <WX type="bool" archive="y">false</WX>
       <data type="bool" archive="y">false</data>
       <pos type="bool" archive="y">true</pos>
     </display-controls>
  </radar>

</PropertyList>
